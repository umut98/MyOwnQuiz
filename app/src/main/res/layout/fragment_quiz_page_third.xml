<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.myownquiz.QuizPageThird"
    android:background="@drawable/error">



    <!-- TODO: Update blank fragment layout -->



    <TextView
        android:id="@+id/yourscore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="105dp"
        android:paddingLeft="10dp"
        android:textColor="#7FFF00"
        android:fontFamily="@font/digital"
        android:textSize="25sp"
        android:text="Your Score Is At The Bottom"
        />
    <TextView
        android:id="@+id/thescore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="160dp"
        android:paddingTop="165dp"
        android:textColor="#ff00fb"
        android:fontFamily="@font/digital"
        android:textSize="40sp"
        android:text="6"
        />
    <TextView
        android:id="@+id/numberofquestions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="180dp"
        android:paddingTop="165dp"
        android:textColor="#ff00fb"
        android:fontFamily="@font/digital"
        android:textSize="40sp"
        android:text="/8"
        />

    <Button
        android:id="@+id/next"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="133dp"
        android:layout_marginTop="240dp"
        android:background="#24cccc"
        android:text="Reset"
        android:textColor="#eec972 "
        android:textSize="22sp"
        android:fontFamily="@font/digital"
        />
</FrameLayout>
